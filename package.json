{
  "name": "f1-stats",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "knex": "knex --knexfile libs/data/knexfile.ts",
    "migrate": "yarn knex migrate:latest",
    "migrate:make": "yarn knex migrate:make -x ts",
    "rollback": "yarn knex migrate:rollback",
    "seed": "yarn knex seed:run",
    "seed:make": "yarn knex seed:make -x ts",
    "db:rebuild": "yarn rollback --all && yarn migrate && yarn seed",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "8.2.3",
    "@nestjs/core": "8.2.3",
    "@nestjs/platform-express": "8.2.3",
    "@willsoto/nestjs-objection": "^6.1.0",
    "csv-parse": "^5.0.3",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "knex": "0.95.14",
    "objection": "3.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.4.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.3",
    "@nrwl/cli": "13.2.2",
    "@nrwl/eslint-plugin-nx": "13.2.2",
    "@nrwl/jest": "13.2.2",
    "@nrwl/linter": "13.2.2",
    "@nrwl/nest": "13.2.2",
    "@nrwl/node": "13.2.2",
    "@nrwl/tao": "13.2.2",
    "@nrwl/workspace": "13.2.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.10",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.0",
    "jest": "27.3.1",
    "lint-staged": ">=10",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.2",
    "ts-jest": "27.0.7",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
