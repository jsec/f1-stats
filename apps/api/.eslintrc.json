{
  "extends": ["../../.eslintrc.json", "airbnb/base", "airbnb-typescript/base", "prettier"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parserOptions": {
        "project": ["apps/api/tsconfig.*?.json"]
      },
      "rules": {
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off"
      }
    }
  ]
}
